<h2>Members</h2>
<div class="row members">
  {% for members in data.members %}
    <div class="col-sm-6 col-lg-3">
      <div class="card mb-4">
        <img class="card-img-top img-fluid" src="img/members/{{ members.imagebw }}" alt="{{members.name}}">
        <div class="card-body">
          <h5 class="card-title"> <a href="{{ members.url }}" class="member-title"> {{ members.name }} </a> </h5>
          <p class="card-text"> {{ members.tenure}} </p>
        </div>
      </div>
    </div>
  {% endfor %}
